{
  "name": "study-mate",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @study-mate/backend dev",
    "dev:setup": "turbo -F @study-mate/backend dev:setup"
  },
  "dependencies": {},
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "packageExtensions": {
      "@convex-dev/auth@*": {
        "dependencies": {
          "cookie": "^0.6.0"
        }
      }
    }
  }
}
